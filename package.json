{
  "name": "yan-hooks",
  "description": "yancode React 业务 Hooks",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dest/index.d.ts",
  "repository": "https://github.com/zdydmn/yan-hooks",
  "homepage": "https://github.com/zdydmn/yan-hooks",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf node_modules pnpm-lock.yaml",
    "init": "pnpm install",
    "dev:docs": "rspress dev",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "test": "jest",
    "build": "rslib build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "pub": "pnpm run build && npm publish"
  },
  "keywords": [
    "yancode",
    "react",
    "hooks"
  ],
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.23.0",
    "@rsbuild/plugin-react": "^1.1.1",
    "@rslib/core": "^0.6.2",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "eslint": "^9.23.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "rspress": "^1.43.11",
    "ts-jest": "^29.3.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  }
}
